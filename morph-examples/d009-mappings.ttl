<#TriplesMap11>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_58519.ID FROM (sport AS t_58519 JOIN student AS t_24778 ON ((t_58519.ID=t_24778.Sport)))" 
	];

	rr:subjectMap [
		rr:template "http://example.com/Sport/{ID}";
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/SportHasStudent>;
		rr:objectMap [ rr:template "http://example.com/Student/{ID}" ];
	].

