# query1 : Triple Block
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

# SELECT ?a ?e ?w 
SELECT *
WHERE { 
	?a <http://mappingpedia.linkeddata.es/vocabulary/email> ?e .
	OPTIONAL { ?a <http://mappingpedia.linkeddata.es/vocabulary/webpage> ?w }
}
